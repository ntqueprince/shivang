        // Function to populate the dropdowns with unique values
        function populateDropdowns() {
            const insuranceCompanySelect = document.getElementById('insurance-company');
            const amendmentTypeSelect = document.getElementById('amendment-type');

            // Skip header row (index 0)
            const insurers = new Set();
            const requirements = new Set();

            // Start from index 1 to skip header
            for (let i = 1; i < insuranceData.length; i++) {
                insurers.add(insuranceData[i][0]);
                requirements.add(insuranceData[i][1]);
            }

            // Convert sets to arrays and sort alphabetically
            const insurersArray = Array.from(insurers).sort();
            const requirementsArray = Array.from(requirements).sort();

            // Populate insurance company dropdown
            insurersArray.forEach(insurer => {
                const option = document.createElement('option');
                option.value = insurer;
                option.textContent = insurer;
                insuranceCompanySelect.appendChild(option);
            });

            // Populate amendment type dropdown
            requirementsArray.forEach(requirement => {
                const option = document.createElement('option');
                option.value = requirement;
                option.textContent = requirement;
                amendmentTypeSelect.appendChild(option);
            });
        }

        // Function to find and display the matching record
        function displayResults() {
            const insurer = document.getElementById('insurance-company').value;
            const requirement = document.getElementById('amendment-type').value;
            
            const noSelectionElement = document.getElementById('no-selection');
            const resultsElement = document.getElementById('results');
            const notFoundElement = document.getElementById('not-found');

            // Hide all result elements initially
            noSelectionElement.classList.add('hidden');
            resultsElement.classList.add('hidden');
            notFoundElement.classList.add('hidden');

            // If no selection made, show the placeholder
            if (!insurer || !requirement) {
                noSelectionElement.classList.remove('hidden');
                return;
            }

            // Find the matching record
            let matchingRecord = null;
            for (let i = 1; i < insuranceData.length; i++) {
                if (insuranceData[i][0] === insurer && insuranceData[i][1] === requirement) {
                    matchingRecord = insuranceData[i];
                    break;
                }
            }

            // If no matching record found, show the not found message
            if (!matchingRecord) {
                notFoundElement.classList.remove('hidden');
                return;
            }

            // Display the matching record
            document.getElementById('result-title').textContent = `${requirement} - ${insurer}`;
            document.getElementById('result-subtitle').textContent = `Requirements for making changes to the policy`;
            
            document.getElementById('endorsement-type').textContent = matchingRecord[2] || 'Not specified';
            document.getElementById('documents').textContent = matchingRecord[3] || 'None required';
            document.getElementById('tat').textContent = matchingRecord[4] || 'Not specified';
            
            // Format charges with a badge
            const chargesElement = document.getElementById('charges');
            const chargesValue = matchingRecord[5] || 'Not specified';
            if (chargesValue.toLowerCase() === 'yes') {
                chargesElement.innerHTML = '<span class="badge badge-yes">Yes</span>';
            } else if (chargesValue.toLowerCase() === 'no') {
                chargesElement.innerHTML = '<span class="badge badge-no">No</span>';
            } else if (chargesValue.toLowerCase() === 'maybe') {
                chargesElement.innerHTML = '<span class="badge badge-maybe">Maybe</span>';
            } else if (chargesValue.toLowerCase() === 'refund') {
                chargesElement.innerHTML = '<span class="badge" style="background-color: #dbeafe; color: #1e40af;">Refund</span>';
            } else if (chargesValue.toLowerCase() === 'deduction') {
                chargesElement.innerHTML = '<span class="badge" style="background-color: #ede9fe; color: #5b21b6;">Deduction</span>';
            } else {
                chargesElement.textContent = chargesValue;
            }
            
            // Format inspection with a badge
            const inspectionElement = document.getElementById('inspection');
            const inspectionValue = matchingRecord[6] || 'Not specified';
            if (inspectionValue.toLowerCase() === 'yes') {
                inspectionElement.innerHTML = '<span class="badge badge-yes">Yes</span>';
            } else if (inspectionValue.toLowerCase() === 'no') {
                inspectionElement.innerHTML = '<span class="badge badge-no">No</span>';
            } else if (inspectionValue.toLowerCase() === 'maybe') {
                inspectionElement.innerHTML = '<span class="badge badge-maybe">Maybe</span>';
            } else {
                inspectionElement.textContent = inspectionValue;
            }
            
            // Handle exceptions
            const exceptionElement = document.getElementById('exceptions');
            const exceptionRow = document.getElementById('exception-row');
            if (matchingRecord[7] && matchingRecord[7].trim() !== '') {
                exceptionElement.textContent = matchingRecord[7];
                exceptionRow.classList.remove('hidden');
            } else {
                exceptionRow.classList.add('hidden');
            }
            
            // Handle declaration format
            const declarationElement = document.getElementById('declaration');
            const declarationRow = document.getElementById('declaration-row');
            if (matchingRecord[8] && matchingRecord[8].trim() !== '') {
                declarationElement.textContent = matchingRecord[8];
                declarationRow.classList.remove('hidden');
            } else {
                declarationRow.classList.add('hidden');
            }
            
            // Show the results
            resultsElement.classList.remove('hidden');
        }

        // Initialize the page
        document.addEventListener('DOMContentLoaded', function() {
            populateDropdowns();
            
            // Add event listeners to dropdowns
            document.getElementById('insurance-company').addEventListener('change', displayResults);
            document.getElementById('amendment-type').addEventListener('change', displayResults);
        });
    </script>
</body>
</html>
